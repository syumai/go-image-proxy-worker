{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "allowJs": true,
    "types": ["node", "@cloudflare/workers-types"],
    "rootDir": "src",
  },
  "include": ["src/**/*"],
}
